{
  "name": "@package/bundler",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "bin": {
    "minibundler": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --clean --dts --no-splitting",
    "dev": "tsup src/index.ts src/cli.ts --format esm --clean --watch --dts --no-splitting"
  },
  "devDependencies": {
    "@types/estree": "^1.0.8",
    "@types/node": "^24.10.4",
    "magic-string": "^0.30.21",
    "tsup": "8.4.0",
    "tsx": "^4.19.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "acorn": "^8.14.0"
  }
}
