{
  "name": "fe-lab",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "react": "turbo run dev --filter=react --filter=@design-system/ui",
    "typescript": "turbo run dev --filter=typescript",
    "next": "turbo run dev --filter=next.js"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "turbo": "^2.4.4"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
